{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Gaigai",
  "scopeName": "source.gaigai",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#constants"
    },
    {
      "include": "#builtin-function-call"
    },
    {
      "include": "#function-call"
    },
    {
      "include": "#identifier"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.gaigai",
          "begin": "//",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.gaigai"
            }
          },
          "end": "$"
        },
        {
          "name": "comment.block.gaigai",
          "begin": "/\\*",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.begin.gaigai"
            }
          },
          "end": "\\*/",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.end.gaigai"
            }
          }
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.gaigai",
          "begin": "\"",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.gaigai"
            }
          },
          "end": "\"",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.gaigai"
            }
          },
          "patterns": [
            {
              "name": "constant.character.escape.gaigai",
              "match": "\\\\([\"'\\\\ntr]|u[0-9A-Fa-f]{4})"
            }
          ]
        },
        {
          "name": "string.quoted.single.gaigai",
          "begin": "'",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.gaigai"
            }
          },
          "end": "'",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.gaigai"
            }
          },
          "patterns": [
            {
              "name": "constant.character.escape.gaigai",
              "match": "\\\\([\"'\\\\ntr]|u[0-9A-Fa-f]{4})"
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.gaigai",
          "match": "\\b-?(?:[0-9]+(?:\\.[0-9]*)?|\\.[0-9]+)(?:[eE][+\\-]?[0-9]+)?\\b"
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.language.gaigai",
          "match": "\\b(?:true|false|null|undefined)\\b"
        }
      ]
    },
    "builtin-function-call": {
      "patterns": [
        {
          "name": "support.function.builtin.gaigai",
          "match": "\\b(?:fromCodePointerer|matVecMultiplier|findLastIndexer|jsonStringifyer|filterObjecter|greaterEqualer|outerProducter|predicateNoter|staticMethoder|base64Decoder|base64Encoder|classChainser|fromEntrieser|matHadamarder|matMultiplier|matSubtractor|methodNameser|regexEscapeer|regexReplacer|spreadApplyer|bindMethoder|diagMatrixer|diagVectorer|instanceOfer|isFunctioner|isInstanceer|matFlattener|regexMatcher|regexSpliter|startsWither|staticKeyser|classBaseer|classNameer|constructer|dateParseer|decrementer|fieldKeyser|findIndexer|incrementer|isIntegerer|kroneckerer|lessEqualer|mapObjecter|matInverser|partitioner|pathDeleter|randomInter|regexTester|staticGeter|staticSeter|substringer|superCaller|urlDecodeer|urlEncodeer|charCodeer)\\b(?=\\s*\\()"
        },
        {
          "name": "support.function.builtin.gaigai",
          "match": "\\b(?:concatener|constanter|deepCloner|deepMerger|delFielder|endsWither|findLaster|getFielder|hasFielder|includeser|isFiniteer|ismatrixer|isObjecter|jsonParser|matIndexer|matPowerer|matReducer|multiplier|notequaler|padStarter|regexAller|setFielder|setIndexer|subclasser|subtractor|transposer|varianceer|coalescer|composeer|countByer|dateAdder|defaultor|deg2rader|entrieser|flatMaper|groupByer|isArrayer|isClasser|matMapper|matScaler|partialer|pathGeter|pathSeter|rad2deger|shuffleer|sigmoider|unshifter|assigner|charAter|clampper|columner|delKeyer|filterer|floorrer|hypotter)\\b(?=\\s*\\()"
        },
        {
          "name": "support.function.builtin.gaigai",
          "match": "\\b(?:includer|inserter|inverter|isoNower|lengther|matAdder|matrixer|matSeter|medianer|memoizer|methoder|numberer|objecter|padEnder|randomer|repeater|replacer|returner|reverser|rotateer|sampleer|stddever|stringer|switcher|unzipper|applyer|chunker|classer|curryer|deleter|divider|equaler|everyer|exister|firster|flatter|greater|indexer|isNaner|lowerer|mainner|omitter|plucker|powerer|rangeer|reducer|rounder|shifter|sliceer|splicer|spliter|thrower|timeser|trimmer|truncer)\\b(?=\\s*\\()"
        },
        {
          "name": "support.function.builtin.gaigai",
          "match": "\\b(?:uniquer|upperer|booler|caller|ceiler|cloner|detter|dotter|expeer|finder|forrer|funner|joiner|keyser|laster|lerper|lesser|logger|mapper|meaner|merger|modder|nander|normer|onceer|picker|pipeer|popper|proder|pusher|ranker|shaper|signer|solver|sorter|sqrter|tanher|tapper|tracer|typeer|valuer|whiler|zipper|abser|adder|ander|coser|dater|geter|haser|inter|loger|maxer|miner|newer)\\b(?=\\s*\\()"
        },
        {
          "name": "support.function.builtin.gaigai",
          "match": "\\b(?:noter|nower|rower|seter|siner|somer|sumer|taner|tryer|xorer|eyer|ifer|orer)\\b(?=\\s*\\()"
        }
      ]
    },
    "function-call": {
      "patterns": [
        {
          "name": "entity.name.function.gaigai",
          "match": "\\b[A-Za-z_][A-Za-z0-9_]*\\b(?=\\s*\\()"
        }
      ]
    },
    "identifier": {
      "patterns": [
        {
          "name": "variable.other.gaigai",
          "match": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
        }
      ]
    }
  }
}
